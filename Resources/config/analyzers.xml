<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="sulu.schema_org.analyzer.organization" class="TheCocktail\Bundle\SuluSchemaOrgBundle\Analyzer\OrganizationAnalyzer">
            <argument type="service" id="sulu.repository.account"/>
            <argument type="service" id="sulu.schema_org.attributes"/>
            <tag name="sulu.schema_org.analyzer" priority=""/>
            <tag name="sulu.context" context="website"/>
        </service>

        <service id="TheCocktail\Bundle\SuluSchemaOrgBundle\Analyzer\StructureAnalyzer">
            <argument type="service" id="sulu.schema_org.attributes"/>
            <tag name="sulu.schema_org.analyzer"/>
            <tag name="sulu.context" context="website"/>
        </service>
    </services>
</container>
